<h2 class="page-header">Edit Page</h2>
<!-- form before FilePond = enctype="multipart/form-data" -->
<form action="/book/<%= book.id %>/edit?_method=PUT" method="POST">
    <%- include('../parcels/_form-fields-book') %>
    <div class="form-row form-row-end button-row">
        <a class="btn btn-danger" href="/book">Cancel</a>
        <button class="btn btn-primary" type="submit">Update</button>
    </div>
</form>